<?xml version="1.0" encoding="utf-8"?>
<resources>




    <declare-styleable name="Theme.Digicade">
        <attr name="homeIcon" format="reference" />
    </declare-styleable>

    <style name="Theme.Digicade" parent="Theme.AppCompat.Light">
        <item name="button">@style/Digicade.Button</item>
        <item name="titulo">@style/Digicade.Titulo</item>
        <item name="detalheTitulo">@style/Digicade.Detalhe.Titulo</item>
        <item name="detalheLabel">@style/Digicade.Detalhe.Label</item>
        <item name="detalheDescricao">@style/Digicade.Detalhe.Descricao</item>
        <item name="itemOsTitulo">@style/Digicade.ItemOs.Titulo</item>
        <item name="itemOsDescricao">@style/Digicade.ItemOs.Descricao</item>
        <item name="itemOsStatus">@style/Digicade.ItemOs.Descricao.Status</item>
    </style>

    <style name="Theme.Digicade.Questionario" parent="Theme.AppCompat.Light">
        <item name="sizeTexto">@dimen/questionario_texto</item>
        <item name="sizeData">@dimen/questionario_data</item>
        <item name="sizeTituloQuestao">@dimen/questionario_titulo_questao</item>
    </style>




    <style name="Theme.Smartisp.Questionario" parent="Theme.AppCompat.Light">
        <item name="sizeTexto">@dimen/questionario_texto</item>
        <item name="sizeData">@dimen/questionario_data</item>
        <item name="sizeTituloQuestao">@dimen/questionario_titulo_questao</item>
    </style>








</resources>